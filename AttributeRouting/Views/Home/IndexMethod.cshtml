@model IEnumerable<Employee>
@{ 
    ViewBag.PageTitle = "Employee List";
}
    <table>
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Department</th>
            </tr>
        </thead>

        <tbody>
            @foreach(var employee in Model)
            {
                <tr>
                    <td>
                        @employee.Id;
                    </td>
                    <td>@employee.Department</td>
                    <td>@employee.Name</td>
                </tr>
            }
        </tbody>
    </table>
